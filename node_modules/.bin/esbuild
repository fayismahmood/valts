#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/ft/New Volume/test/ts/node_modules/esbuild/bin/node_modules:/media/ft/New Volume/test/ts/node_modules/esbuild/node_modules:/media/ft/New Volume/test/ts/node_modules:/media/ft/New Volume/test/node_modules:/media/ft/New Volume/node_modules:/media/ft/node_modules:/media/node_modules:/node_modules:/media/ft/New Volume/test/ts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/ft/New Volume/test/ts/node_modules/esbuild/bin/node_modules:/media/ft/New Volume/test/ts/node_modules/esbuild/node_modules:/media/ft/New Volume/test/ts/node_modules:/media/ft/New Volume/test/node_modules:/media/ft/New Volume/node_modules:/media/ft/node_modules:/media/node_modules:/node_modules:/media/ft/New Volume/test/ts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
