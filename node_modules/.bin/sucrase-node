#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/ft/New Volume/test/ts/node_modules/sucrase/bin/node_modules:/media/ft/New Volume/test/ts/node_modules/sucrase/node_modules:/media/ft/New Volume/test/ts/node_modules:/media/ft/New Volume/test/node_modules:/media/ft/New Volume/node_modules:/media/ft/node_modules:/media/node_modules:/node_modules:/media/ft/New Volume/test/ts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/ft/New Volume/test/ts/node_modules/sucrase/bin/node_modules:/media/ft/New Volume/test/ts/node_modules/sucrase/node_modules:/media/ft/New Volume/test/ts/node_modules:/media/ft/New Volume/test/node_modules:/media/ft/New Volume/node_modules:/media/ft/node_modules:/media/node_modules:/node_modules:/media/ft/New Volume/test/ts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sucrase/bin/sucrase-node" "$@"
else
  exec node  "$basedir/../sucrase/bin/sucrase-node" "$@"
fi
